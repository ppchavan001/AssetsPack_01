{
  "presets": [
    "@babel/preset-typescript",
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": "6.10"
        },
        "modules": "cjs"
      }
    ]
  ],
  "plugins": [
    "@aidaspa/babel-plugin-uts",
    [
      "@babel/plugin-proposal-decorators",
      {
        "legacy": true
      }
    ]
  ],
  "comments": true, // output comments in generated code
  "compact": false, // do not remove whitespace in generated code
  "minified": false, // generate minified code
  "retainLines": true, // do not retain line numbers in generated code
  // "sourceFileName": "", // specify the filename to use for the source code
  "sourceMaps": true, // do not generate source maps
  // "sourceRoot": "", // specify the root path for the source code
  // "auxiliaryCommentBefore": "", // add a comment to the top of the generated code
  // "auxiliaryCommentAfter": "" // add a comment to the bottom of the generated code,
  "ignore": [
    "**/*.d.ts"
  ]
}
